{
  "name": "remark-block-containers",
  "version": "1.0.0",
  "description": "remark 插件-用于创建自定义容器，允许添加标题、id或样式类",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "npm run prebuild && tsc",
    "lint": "tsc --noEmit && eslint 'src/**' --ext .js,ts",
    "lint-fix": "npm run lint -- --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leezhian/remark-block-containers.git"
  },
  "bugs": {
    "url": "https://github.com/leezhian/remark-block-containers.git/issues"
  },
  "homepage": "https://github.com/leezhian/remark-block-containers",
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "remark",
    "remark-plugin",
    "remark-block",
    "remark-containers",
    "remark-block-containers"
  ],
  "sideEffects": false,
  "author": "kim",
  "license": "MIT",
  "dependencies": {
    "nanoid": "^5.0.1",
    "unist-util-find-after": "^5.0.0",
    "unist-util-find-between-all": "^1.0.4",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.5",
    "@types/mdast": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "mdast": "^3.0.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "unified": "^11.0.3"
  }
}
